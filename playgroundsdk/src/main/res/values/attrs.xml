<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DoButton">
        <attr name="tint" format="color"/>
        <attr name="query" format="string"/>
    </declare-styleable>

    <declare-styleable name="SuisseTextView">
        <attr name="type" format="enum">
            <enum name="regular" value="0"/>
            <enum name="bold" value="1"/>
            <enum name="black" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="PlaygroundWidget">
        <attr name="num_actions_widget" format="integer"/>
        <attr name="query_widget" format="string"/>
        <attr name="latitude_widget" format="string"/>
        <attr name="longitude_widget" format="string"/>
    </declare-styleable>

</resources>